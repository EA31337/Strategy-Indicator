/**
 * @file
 * Defines default strategy parameter values for the given timeframe.
 */

// Defines indicator's parameter values for the given pair symbol and timeframe.
struct Indi_Indicator_Params_{{ Timeframe }} : IndicatorIndiParams {
  Indi_Indicator_Params_{{ Timeframe }}() : IndicatorIndiParams(indi_indi_defaults, PERIOD_{{ Timeframe }}) {
    shift = {{ Indicator_Indi_Indicator_Shift | default(0) }};
  }
} indi_indi_{{ Timeframe | lower }};

// Defines strategy's parameter values for the given pair symbol and timeframe.
struct Stg_Indicator_Params_{{ Timeframe }} : StgParams {
  // Struct constructor.
  Stg_Indicator_Params_{{ Timeframe }}() : StgParams(stg_indi_defaults) {
    lot_size = {{ Indicator_LotSize | default(0) }};
    signal_open_method = {{ Indicator_SignalOpenMethod | default(0) }};
    signal_open_filter_method = {{ Indicator_SignalOpenFilterMethod | default(1) }};
    signal_open_level = (float){{ Indicator_SignalOpenLevel | default(0) }};
    signal_open_boost = {{ Indicator_SignalOpenBoostMethod | default(0) }};
    signal_close_method = {{ Indicator_SignalCloseMethod | default(0) }};
    signal_close_level = (float){{ Indicator_SignalCloseLevel | default(0) }};
    price_stop_method = {{ Indicator_PriceStopMethod | default(0) }};
    price_stop_level = (float){{ Indicator_SignalCloseLevel | default(1) }};
    tick_filter_method = {{ Indicator_TickFilterMethod  | default(1) }};
    max_spread = {{ Indicator_MaxSpread | default(0) }};
  }
} stg_indi_{{ Timeframe | lower }};
